<p id="notice"><%= notice %></p>
<div class="col-lg-8 col-md-offset-2">
	
<p>
  <strong>标题:</strong>
  <%= @activity_post.title %>
</p>

<p>
  <strong>内容:</strong>
  <%= raw @activity_post.content %>
</p>

<p>
  <strong>修改时间:</strong>
  <%= @activity_post.updated_at.to_s[0,19] %>
</p>

<%= link_to '编辑文章', edit_activity_post_path(@activity_post),class:"btn btn-mini btn-primary" %> |
<%= link_to '返回文章列表', activity_posts_path,class:"btn btn-mini btn-primary" %>
</div>
