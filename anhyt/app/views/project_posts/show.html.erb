<p id="notice"><%= notice %></p>
<div class="col-lg-8 col-md-offset-2">
  <p>
    <strong>标题:</strong>
    <%= @project_post.title %>
  </p>

  <p>
    <strong>内容:</strong>
    <%= raw @project_post.content %>
  </p>
  
  <p>
    <strong>更新时间:</strong>
    <%= @project_post.created_at.to_s[0,19] %>
  </p>
  
  <%= link_to '编辑项目', edit_project_post_path(@project_post),class:"btn btn-mini btn-primary" %> |
  <%= link_to '返回项目列表', project_posts_path,class:"btn btn-mini btn-primary" %>
</div>
